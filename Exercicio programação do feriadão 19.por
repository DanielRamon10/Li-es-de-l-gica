programa
{
	
	funcao inicio()
	{
		inteiro a, b, troca
		escreva("Digite o valor (númerico) da variável A:")
		leia(a)

		escreva("Digite o valor (númerico) da variável B:")
		leia(b)

		troca = a 
		a = b
		b = troca

		escreva("O novo valor de A é: " + a)
		escreva("\nO novo valor de B é: " + b)
	}
}
